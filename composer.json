{
    "name": "veekthoven/laravel-inertia-toast",
    "description": "Beautiful toast notifications for Laravel + Inertia.js applications. Fluent PHP API, multi-toast support, redirect-safe, with Vue 3 and React adapters.",
    "keywords": ["laravel", "inertia", "toast", "notifications"],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Abbah Nkoms"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "inertiajs/inertia-laravel": "^2.3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "InertiaToast\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "InertiaToast\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InertiaToast\\InertiaToastServiceProvider"
            ],
            "aliases": {
                "Toast": "InertiaToast\\Facades\\Toast"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
