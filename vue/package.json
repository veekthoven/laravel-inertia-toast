{
  "name": "@laravel-inertia-toast/vue",
  "version": "0.1.0",
  "description": "Vue 3 toast components for Laravel Inertia Toast",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "@inertiajs/vue3": "^2.3.3",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@inertiajs/vue3": "^2.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.4.0",
    "vite": "^6.0.0",
    "vue": "^3.5.0",
    "vue-tsc": "^2.2.0"
  },
  "keywords": [
    "laravel",
    "inertia",
    "toast",
    "vue",
    "notifications"
  ]
}
